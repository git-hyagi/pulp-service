#set xlabel "Values"
set title "Tasks"
set terminal postscript enhanced color landscape
set output graph_file

#set xdata time
#set timefmt "%Y-%m-%dT%H:%M:%S.%zZ"
#set style data lines

set style data histogram
set style histogram rowstacked
set boxwidth 0.75
#set style histogram cluster gap 1
set style fill solid 1.0 border -1.0       # Fill style for the boxes

# Fine control of the major (labelled) tics on the x axis
set xtics rotate by -90
set grid ytics lt rgb "gray"

#plot "/tmp/a" u 1:2
plot data_file using 2:xtic(1) ti 'runing' lt rgb 'green', '' using 3 ti 'waiting blocked' lt rgb 'red', '' using 4 ti 'waiting unblocked' lt rgb 'blue'

#pause -1
